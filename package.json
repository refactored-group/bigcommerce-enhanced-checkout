{
  "name": "automatic-ffl-react-iframe",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.121",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vis.gl/react-google-maps": "^1.5.0",
    "dotenv": "^16.4.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run generate-config && npm run minify-js && PUBLIC_URL=/big-commerce-enhanced-checkout react-scripts build",
    "build:dev": "npm run generate-config && npm run minify-js && react-scripts build",
    "generate-config": "sed -e \"s|FFL_STORE_ENDPOINT: ''|FFL_STORE_ENDPOINT: '${FFL_STORE_ENDPOINT}'|g\" -e \"s|FFL_IFRAME_URL: ''|FFL_IFRAME_URL: '${FFL_IFRAME_URL}'|g\" config.template.js > public/js/config.js",
    "minify-js": "terser public/js/bigcommerce.js -o public/js/bigcommerce.min.js --compress --mangle",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "sass": "^1.81.0",
    "tailwindcss": "^3.4.17",
    "terser": "^5.39.0"
  }
}
